{
    "compilerOptions": {
        "outDir": "dist",
        "module": "commonjs",
        "target": "es2019",
        "esModuleInterop": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "rootDir": "src",
        "baseUrl": "src",
        "paths": {
            "@data/*":["data/*"],
            "@domain/*":["domain/*"],
            "@infra/*":["infra/*"],
            "@main/*":["main/*"],
            "@presentation/*":["presentation/*"],
            "@validation/*":["validation/*"]
        }
    },
    "include": [
        "src"
    ],
    "exclude": []
}